<div class="booyah-box col-10 offset-1">
  <h1>Displace to Gas Calculator</h1>
  <%= simple_form_for @well do |f| %>
    <%= f.input :toptvd %>
    <%= f.input :bottomtvd %>
    <%= f.input :gasgradient %>
    <%= f.input :influxtvd%>
    <%= f.input :influxpressure %>
    <br />
    <%= f.submit 'Calculate', class: 'btn btn-primary' %>
  <% end %>
</div>